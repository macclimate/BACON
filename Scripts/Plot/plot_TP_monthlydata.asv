%% Investigate cumulative differences in NEP for different times of year
bot = [60  61  121 181 241 301]';
top = [120 180 180 240 300 365]';

for kk = 1:1:length(bot)
MJ_cum_PPT2004 = cumsum(PPT_d_sum(bot(kk):top(kk),3));    
