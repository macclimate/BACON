clf;
subplot(4,1,1); plot(MJ_cum_NEP2005,'k');
hold on;        plot(MJ_cum_NEP2006,'b');
                plot(MJ_cum_NEP2004,'r');
axis([0 (top(kk)-bot(kk)) -100 200]); grid on;                
                
