hold on;        plot(MJ_SM2006,'b');
                plot(MJ_SM2004,'r');
axis([0 (top(kk)-bot(kk)) 0 0.3])                   
                
subplot(4,1,4); plot(MJ_Ta2005,'k');
hold on;        plot(MJ_Ta2006,'b');
                plot(MJ_Ta2004,'r');                
axis([0 (top(kk)-bot(kk)) -20 30])                   

