figure(1); clf;
plot(Ts5(10225:16290,1), chamber_data(:,15), 'r.') 
hold on;
plot(Ts5(10225:16290,1), chamber_data(:,25), 'b.')
plot(Ts5(10225:16290,1), chamber_data(:,35), 'c.')
plot(Ts5(10225:16290,1), chamber_data(:,45), 'g.')
